version 41
__lua__
function _init()
	x = 64 y = 64
end

function _update60()
	if (btn(⬅️)) then x -= 1 end
	if (btn(➡️)) then x += 1 end
	if (btn(⬆️)) then y -= 1 end
	if (btn(⬇️)) then y += 1 end
end

function _draw()
	cls(1)
	spr(1, x, y)
end


